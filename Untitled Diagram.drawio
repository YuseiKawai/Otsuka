        <mxCell id="9UxpXEo3Pc5CRail3H-m-5" value="スキル" style="rounded=0;whiteSpace=wrap;html=1;strokeColor=#999999;fontSize=10;fontFamily=Montserrat;fontSource=https%3A%2F%2Ffonts.googleapis.com%2Fcss%3Ffamily%3DMontserrat;fontColor=#808080;" vertex="1" parent="9UxpXEo3Pc5CRail3H-m-1">
